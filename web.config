<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
		<rule name="redirect: mpiresearch" stopProcessing="true">
			<match url="(.*)" />
    			<conditions logicalGrouping="MatchAll">
        			<add input="{HTTP_HOST}" pattern="^(mpiresearch\.com|www\.mpiresearch\.com)$" />
        			<add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
    			</conditions>
			<action type="Redirect" url="https://www.criver.com/insights/charles-river-completes-acquisition-mpi-research" appendQueryString="false" />
        	</rule>
		    <rule name="redirect: atlanbio" stopProcessing="true">    <match url="(.*)" />
   <conditions logicalGrouping="MatchAll">
        <add input="{HTTP_HOST}" pattern="^(atlanbio\.com|www\.atlanbio\.com)$" />
        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
    </conditions><action type="Redirect" url="https://www.criver.com/atlanbio-now-part-charles-river" appendQueryString="false" />
		    </rule>
		    <rule name="redirect: citoxlab" stopProcessing="true">    <match url="(.*)" />
   <conditions logicalGrouping="MatchAll">
        <add input="{HTTP_HOST}" pattern="^(citoxlab\.com|www\.citoxlab\.com|citoxlab\.fr|www\.citoxlab\.fr)$" />
        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
    </conditions><action type="Redirect" url="https://www.criver.com/citoxlab-now-part-charles-river" appendQueryString="false" />
		    </rule>

 </rules>
        </rewrite>
    </system.webServer>
</configuration>
